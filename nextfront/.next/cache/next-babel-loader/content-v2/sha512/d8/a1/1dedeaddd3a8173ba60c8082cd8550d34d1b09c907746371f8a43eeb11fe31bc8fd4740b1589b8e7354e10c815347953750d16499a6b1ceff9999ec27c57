pobj }, () => console.log(this.state.addStatus));\n    };\n\n    handleAddTask = e => {\n        e.preventDefault()\n        fetch('http://127.0.0.1:5000/tasks/create', {\n            method: 'POST',\n            headers: { 'Content-type': 'application/json' },\n            body: JSON.stringify(this.state)\n        }).then(r => r.json())\n            .then(res => {\n                this.setState({\n                    tasks: res\n                })\n            })\n        this.setState({ show: false })\n    }\n\n    handleAddStatus = e => {\n        e.preventDefault()\n        fetch('http://127.0.0.1:5000/tasks/status/create', {\n            method: 'POST',\n            headers: { 'Content-type': 'application/json' },\n            body: JSON.stringify(this.state.addStatus)\n        }).then(r => r.json())\n            .then(res => {\n                this.setState({\n                    allStatus: res\n                })\n            })\n        this.setState({ statusshow: false }, console.log(\"this.state==>\", this.state))\n    }\n\n    handleDeleteStatus = e => {\n\n        e.preventDefault()\n        fetch(`http://127.0.0.1:5000/tasks/status/delete/${this.state.deleteStatus}`, {\n            method: 'POST'\n\n        })\n            .then(r => r.json())\n            .then(res => {\n                this.setState({\n                    allStatus: res\n                })\n            })\n        this.setState({ deletestatusshow: false }, console.log(\"this.state==>\", this.state))\n    }\n\n\n    render() {\n        const handleClose = () => this.setState({ show: false });\n        const handleAddStatusClose = () => this.setState({ statusshow: false });\n        const handleDeleteStatusClose = () => this.setState({ deletestatusshow: false });\n        return (\n            <div style={{ backgroundImage: \"linear-gradient(to right, #4880EC, #019CAD)\" }}>\n                <div style={{ textAlign: 'right' }}>\n                    <Button variant=\"success\" onClick={(